{
  "name": "viterts",
  "private": true,
  "version": "0.2.1",
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "yarn run lint:ts",
    "lint:ts": "tsc && yarn lint:eslint",
    "lint:eslint": "eslint 'src/**/*.{ts,tsx}'",
    "format:ts": "prettier --write 'src/**/*.{ts,tsx}' && yarn lint:eslint --fix",
    "format": "yarn run format:ts",
    "format:check": "prettier -c 'src/**/*.{ts,tsx}'",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@codemirror/autocomplete": "^6.6.1",
    "@codemirror/lang-javascript": "^6.1.8",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lint": "^6.2.1",
    "@codemirror/state": "^6.2.0",
    "@codemirror/view": "^6.11.0",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.1",
    "@reduxjs/toolkit": "^1.8.5",
    "@uiw/codemirror-theme-bbedit": "^4.20.2",
    "@uiw/codemirror-theme-darcula": "^4.20.2",
    "@uiw/react-codemirror": "^4.20.2",
    "antd": "^5.1.0",
    "bootstrap": "^5.2.0",
    "cm6-graphql": "^0.0.5",
    "codemirror": "^6.0.1",
    "eol-converter-cli": "^1.0.8",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "firebase": "^9.20.0",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "react-animations": "^1.0.0",
    "react-codemirror": "^1.0.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.10.0",
    "react-transition-group": "^4.4.5",
    "styled-components": "^6.0.0-rc.1",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/codemirror": "^5.60.7",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-istanbul": "^0.30.1",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.6",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "istanbul": "^0.4.5",
    "jest": "^29.0.2",
    "jest-environment-jsdom": "^29.0.2",
    "prettier": "^2.7.1",
    "sass": "^1.54.8",
    "typescript": "^4.6.4",
    "vite": "^4.0.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-plugin-static-copy": "^0.15.0",
    "vitest": "^0.30.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  }
}
